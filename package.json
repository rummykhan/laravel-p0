{
  "name": "rummykhan/cdk-app",
  "version": "0.1.0",
  "bin": {
    "rummykhan/cdk-app": "bin/app.js"
  },
  "scripts": {
    "clean": "find . -type f -name \"*.d.ts\" -delete && find . -type f -name \"*.js\" -delete",
    "full-clean": "rm -rf node_modules dist build && rm -rf package-lock.json && npm run clean",
    "clean-build": "npm run full-clean && npm install && npm run build",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "aws-cdk": "2.1016.1",
    "ts-node": "^10.9.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.196.0",
    "constructs": "^10.0.0",
    "typescript": "~5.6.3"
  }
}